  </PropertyGroup>
  <PropertyGroup Condition="'$(FridaV8)'=='Enabled'">
    <FridaCoreValaFlags>-D HAVE_V8 $(FridaCoreValaFlags)</FridaCoreValaFlags>
  </PropertyGroup>
